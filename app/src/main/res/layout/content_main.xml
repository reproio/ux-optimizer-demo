    </LinearLayout>
    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/messageView" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="40dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="40dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/titleView"/>
    <ImageView
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:layout_width="match_parent"
            android:layout_height="267dp" app:srcCompat="@drawable/spring_image"
            android:id="@+id/productView" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/button2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/messageView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="40dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="40dp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.15"/>

